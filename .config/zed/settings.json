      "source": "extension",
      "enabled": true,
      "settings": {
        "context7_api_key": "${CONTEXT7_API_KEY}"
      }
    }
  },
  "ssh_connections": [
    {
      "host": "wk",
      "args": [],
      "projects": [
        {
          "paths": ["/home/wukaige/project/ai"]
