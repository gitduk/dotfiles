% envs

$ host: { awk '!/^#/ && NF { if (NF>=2) printf "%s\n%s\n", $1, $2; else print $1 }' /etc/hosts; echo "10.0.2.2"; [[ -f "$HOME/.ssh/known_hosts" ]] && grep -oE '([0-9]{1,3}\.){3}[0-9]{1,3}|\[[0-9a-fA-F:]+\]' "$HOME/.ssh/known_hosts" | sed 's/\[//;s/\]//'; } | sort -u
$ dev: networkctl list | awk '/ether/ && /configured/ {print $2}'

