! ######################### Xresources #########################
*background: #1E1E2E
*foreground: #D9E0EE

! Gray
*color0: #6E6C7E
*color8: #988BA2

! Red
*color1: #F28FAD
*color9: #F28FAD

! Green
*color2: #ABE9B3
*color10: #ABE9B3

! Yellow
*color3: #FAE3B0
*color11:  #FAE3B0

! Blue
*color4: #96CDFB
*color12: #96CDFB

! Maguve
*color5: #DDB6F2
*color13: #DDB6F2

! Pink
*color6: #F5C2E7
*color14: #F5C2E7

! Whites
*color7: #C3BAC6
*color15: #D9E0EE

! cursor
Xcursor.theme: Vimix
Xcursor.size: 16

! ############################ URxvt ############################

! fonts
URxvt.font: xft:MesloLGS NF:style=Regular:size=9:antialias=true,xft:LXGW WenKai Mono:regular:size=10:minspace=true:antialias=true
URxvt.boldFont: xft:MesloLGS NF:style=Bold:size=9:antialias=true,xft:LXGW WenKai Mono:Bold:size=10:minspace=true:antialias=true
URxvt.italicFont: xft:MesloLGS NF:style=Italic:size=9:antialias=true,xft:LXGW WenKai Mono:Italic:size=10:minspace=true:antialias=true
URxvt.boldItalicfont: xft:MesloLGS NF:style=BoldI talic:size=9:antialias=true,xft:LXGW WenKai Mono:BoldItalic:size=10:minspace=true:antialias=true

! scrollbar setting
URxvt.scrollBar:          false
URxvt.scrollstyle:        next
URxvt.scrollBar_right:    false
URxvt.scrollBar_floating: true
URxvt.scrollTtyOutput:    true
URxvt.scrollWithBuffer:   true
URxvt.scrollTtyKeypress:  true

! secondary
URxvt.secondaryScreen:    1
URxvt.secondaryScroll:    0
URxvt.secondaryWheel:     1

! border
URxvt.internalBorder:       15
URxvt.externalBorder:       true
URxvt.borderLess:           false

! space
URxvt.letterSpace:          0
URxvt.lineSpace:            0

URxvt.geometry:             92x24

! curosr
URxvt.cursorBlink: true
URxvt.cursorUnderline: false

! other
URxvt.loginShell:           true
URxvt.intensityStyles:      false
URxvt.allow_bold:           false
URxvt.saveLines:            8000
URxvt.urgentOnBell:         true
URxvt.depth:                24
URxvt.shading:              100
URxvt.multichar_encoding:   utf-8
URxvt.preeditType:          Root
URxvt.inputMethod:          fcitx
URxvt.dynamicColors:        on

! Keybinds
!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
! Disable printing the terminal contents when pressing PrintScreen.
URxvt.print-pipe: "cat > /dev/null"

! ########################### Plugins ###########################
URxvt*perl-lib:                   /home/wukaige/.urxvt
URxvt.perl-ext-common:            default,matcher,clipboard,selection-to-clipboard,url-select,vim-scrollback,selection=to-clipboard

! url-select
URxvt.url-select.underline:       true
URxvt.url-select.launcher:        /usr/bin/google-chrome-stable
! URxvt.keysym.M-u:                 perl:url-select:select_next

! clipboard
URxvt.clipboard.autocopy:         true
URxvt.clipboard.copycmd:          xclip -i -selection clipboard
URxvt.clipboard.pastecmd:         xclip -o -selection clipboard
URxvt.keysym.Shift-Control-V:     perl:clipboard:paste
URxvt.keysym.Shift-Control-C:     perl:clipboard:copy
URxvt.iso14755:                   false
URxvt.iso14755_52:                false

! vim-scrollback
URxvt.keysym.M-d:                 perl:vim-scrollback:start
URxvt.vim-scrollback-paste:       C-p
URxvt.vim-scrollback-bg:          16
URxvt.vim-scrollback-fg:          10
URxvt.vim-scrollback.pattern.1:   \\B(/\\S+?):(\\d+)(?=:|$)
URxvt.vim-scrollback.launcher.1:  nvim +$2 $1
URxvt*urlLauncher:                /usr/bin/google-chrome-stable

! matcher
!! urxvt.matcher.rend.1: -Uline
URxvt.keysym.C-u:                 matcher:select
URxvt.matcher.button:             1
URxvt.keysym.M-semicolon:         perl:matcher:select
URxvt.matcher.launcher.0:         /usr/bin/google-chrome-stable
URxvt.matcher.pattern.1:          \\B(/(home|usr|etc)\\S+[a-zA-Z])
URxvt.matcher.launcher.1:         /usr/bin/urxvt -e nvim $1
URxvt.matcher.pattern.2:          \\B(/\\S+\\.(jpg|png|jpeg)$)
URxvt.matcher.launcher.2:         /usr/bin/feh $1
URxvt.matcher.pattern.3:          \\B(/\\S+\\.pdf$)
URxvt.matcher.launcher.3:         /usr/bin/evince $1
URxvt.matcher.pattern.4:          \\B(/\\S+\\.(zip|gz|deb|xz)$)
URxvt.matcher.pattern.5:          \\B(/\\S+\\.md$)
URxvt.matcher.launcher.5:         /usr/bin/typora $1

